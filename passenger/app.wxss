@import './pages/styles/base.wxss';
@import './pages/styles/main.wxss'; 

/**app.wxss**/

page {
  width: 100%;
  height: 100%;
  font-family: 'PingFang-SC-Regular';
  font-size: 28rpx;
  background: #f2f4f5;
}

/** by youyou start **/

/* 登录、注册 start */
.login-wrap{
  width:550rpx;
  margin-top:110rpx;
}
.login-form-title{
  color:#333;
  font-size:80rpx;
}
.login-form-btn{
  width:550rpx;
  height:100rpx; 
  line-height:100rpx;
  font-weight:bold;
  border-radius: 50rpx; 
  font-size:36rpx;
  color:#fff;
}
.login-form-input{
  border-bottom:1px solid #CCCCCC;
  font-size:28rpx;
