/** by youyou end **/

/* 公用最外层 */

.container {
  width: 100%;
  height: 100%;
  color: #333;
  box-sizing: border-box;
  padding: 20rpx;
  position: relative;
}

/* 公用最外层end */

/* 公用块 */

.main-wrap {
  background: #fff;
  border-radius: 4rpx;
  box-shadow: 0 5px 10px rgba(0, 0, 0, 0.06);
  overflow: hidden;
}

.main-wrap.ad-pad {
  padding: 30rpx 30rpx 22rpx;
}

.main-wrap + .main-wrap {
  margin-top: 20rpx;
}

/* 公用外层end */

.dp-flex {
  display: flex;
  align-items: center;
}

.dp-flex.flex-one > view {
  flex: 1;
}

.dp-flex.jc-sb {
  justify-content: space-between;
}
.dp-flex.jc-cen {
  justify-content: center;
}

/* 公用样式end */

/* 导航区 */

.main-nav {
  color: #666;
  padding: 57rpx 40rpx 37rpx;
}

.main-nav > view {
  font-size: 24rpx;
  text-align: center;
}

.main-nav > view:not(:last-of-type) {
  margin-right: 80rpx;
}

.main-nav > view image {
  width: 60rpx;
  height: 28rpx;
  margin-bottom: 25rpx;
}

/* 导航区end */

/* 表单样式 */

.main-tit {
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
}
.main-tit.ad-b-bor {
  padding-bottom: 30rpx;
  border-bottom: 1px solid #ccc;
}
.main-form {
}

.main-form .item {
  padding: 25rpx 0;
  border-bottom: 1px solid #ccc;
}
.main-form .item.no-bor-b {
  border-bottom: 0;
}
.main-form .lab {
  width: 140rpx;
  color: #666;
  margin-right: 40rpx;
}

.main-form .sub-lab {
  font-size: 24rpx;
  color: #999;
}

.main-form input {
}

.main-form .info {
  /* color: #BBB; */
}

/* 搜索框 */

.search {
  position: relative;
}

.search input {
  text-align: center;
  background: #f2f2f2;
  border-radius: 30rpx;
}

/* placeholder */

.ph {
  color: #bbb;
}

/* 搜索框end */

/* 主按钮 */

.btn-wrap {
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
  padding: 20rpx;
  box-sizing: border-box;
}

.main-btn {
  width: 100%;
  font-size: 32rpx;
  color: #fff;
  text-align: center;
  background: #f1604f;
  border-radius: 4rpx;
  padding: 10rpx;
}

.tips {
  font-size: 24rpx;
  color: #666;
  text-align: center;
  margin-bottom: 30rpx;
}
.tips.sub {
  color: #fff;
}
/* 主按钮end */

/* 遮罩 */
.mask {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  background: rgba(0, 0, 0, .5);
  padding: 20rpx;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 500;
}
/* 遮罩end */

/* 表单样式end */

/* 自定义picker start */
.custom-picker-wrap {
  position: absolute;
  left: 0;
  top: 0;
}
.custom-picker {
  width: 100%;
  position: fixed;
  bottom: -300px;
  background: #FFF;
  z-index: 999;
}
.custom-picker.enter {
  bottom: 0;
  -webkit-transition: bottom .3s ease-in-out;
  transition: bottom .3s ease-in-out;
}
.custom-picker.leave {
  bottom: -300px;
  -webkit-transition: bottom .3s ease-in-out;
  transition: bottom .3s ease-in-out;
}
.custom-picker-column {
  text-align: center;
  line-height: 50px;
}

.custom-picker-head {
  display: flex;
  padding: 24rpx;
  background: #FFF;
  border-bottom: 1px solid #EBEBEB;
}
.custom-picker-head text {
  flex: 1;
}
.custom-picker-head .confirm {
  text-align: right;
}

/* 自定义picker end */
