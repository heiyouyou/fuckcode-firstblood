  border-radius: 32rpx;
  padding-left:68rpx; 
  font-size:14px;
}
.filter-content-box{
  margin-bottom:80px;
}
.filter-tab{
  margin-top:12rpx;
  height:96rpx;
}
.filter-type-btn{
  position:fixed;
  bottom:80rpx;
  right:48rpx;
  min-width:160rpx;
  height:60rpx;
  text-align:center;
  line-height:60rpx; 
  background:rgba(0,0,0,.5);
  border-radius: 30px 

}
.filter-type-screen{
