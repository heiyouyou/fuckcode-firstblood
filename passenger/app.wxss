@import './pages/styles/base.wxss';
@import './pages/styles/main.wxss';

/**app.wxss**/

page {
  width: 100%;
  height: 100%;
  font-family: 'PingFang-SC-Regular';
  font-size: 28rpx;
  background: #f2f4f5;
}

map {
  width: 100%;
}

/** by youyou start **/

/* 登录、注册 start */

.login-wrap {
  width: 550rpx;
  margin-top: 110rpx;
}

.login-form-title {
  color: #333;
  font-size: 80rpx;
}

.login-form-btn {
  width: 550rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-weight: bold;
  border-radius: 50rpx;
  font-size: 36rpx;
  color: #fff;
}

.login-form-input {
  border-bottom: 1px solid #ccc;
  font-size: 28rpx;
  color: #333;
  line-height: 18px;
}

.register-next-btn {
  background: #fff;
  color: #f1604f;
  border: 1px solid #f1604f;
}

.login-placeholder {
  text-indent: 15px;
  color: #bbb;
  font-size: 14px;
}

/* 登录、注册 end */

/* 弹出层 start */

.mask-no-padding {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  left: 0;
  top: 0;
  z-index: 500;
}

.mask-title {
  height: 120rpx;
  padding: 44rpx 30rpx;
  box-sizing: border-box;
}

.mask-item {
  height: 160rpx;
  padding-left: 40rpx;
  padding-top: 44rpx;
  box-sizing: border-box;
}

.mask-item.active {
  background: rgba(0, 0, 0, 0.1);
}

/* 弹出层 end */

/* TAB切换 start */

.tab-head {
  height: 88rpx;
  color: #999;
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  z-index:99;
  background: rgba(255, 255, 255, 1);
  box-shadow: 4px px 10px rgba(0, 0, 0, 0.1);
}

.tab-body {
  margin-top: 108rpx;
}

/* TAB切换 end */

.input-des-text {
  min-width: 130rpx;
  box-sizing: border-box;
  margin-right: 37rpx;
  display: inline-block;
}

.result-img {
  width: 388rpx;
  height: 295rpx;
}

/* 搜索过滤 start */

.filter-box {
  padding-top: 24rpx;
  position: fixed;
  width: 100%;
  z-index: 222;
}

.search-icon {
  position: absolute;
  left: 13px;
  top: 10px;
}

.top-search-input {
  height: 64rpx;
  background: #ebebeb;
  border-radius: 32rpx;
  padding-left: 68rpx;
  font-size: 14px;
}

.filter-content-box {
  margin-bottom: 80px;
  margin-top: 108px;
}

.filter-tab {
  margin-top: 12rpx;
  height: 96rpx;
}

.filter-type-btn {
  position: fixed;
  bottom: 80rpx;
  right: 48rpx;
  min-width: 160rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  background: rgba(0, 0, 0, 0.5);
  border-radius: 30px;
}

.filter-type-screen {
  width: 26rpx;
  height: 31rpx;
}

.route-tag {
  height: 40rpx;
  line-height: 40rpx;
  box-sizing:border-box;
  border-radius: 4rpx;
  text-align: center;
  display: inline-block;
  padding: 0 4px;
  margin-left: 10rpx;
}

.route-tag-img {
  width: 90rpx;
  height: 34rpx;
}

.content-padding {
  padding: 50rpx 30rpx 0 37rpx;
}

.content-item {
  border-radius: 2px;
}

.item-btn {
  min-width: 140rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  box-sizing: border-box;
  border: 1px solid #666;
  border-radius: 4rpx;
  font-size: 14px;
  margin-left: 5px;
  padding: 0 3px;
}

.watermark {
  position: absolute;
  right: 0;
  top: -10px;
}

.watermark-img {
  width: 200rpx;
  height: 151rpx;
}

/* 搜索过滤 end */

.order-tag {
  height: 36rpx;
  line-height: 36rpx;
  border-radius: 4rpx;
  text-align: center;
  display: inline-block;
  padding: 2rpx 4rpx;
}

.map-box {
  width: 100%;
  height: 400rpx;
}

.map-status-box {
  position: absolute;
  left: 0;
  bottom: 50rpx;
  min-width: 224rpx;
  height: 76rpx;
  line-height: 76rpx;
  text-align: center;
  background: rgba(255, 255, 255, 1);
  border-radius: 0 38rpx 38rpx 0;
  box-shadow: 4px 0px 8px rgba(0, 0, 0, 0.06);
  z-index: 33;
}

.comment-star {
  margin-right: 34rpx;
  width: 50rpx;
  height: 48rpx;
}

.comment-star {
  margin-right: 34rpx;
  width: 50rpx;
  height: 48rpx;
}

.comment-star:last-child {
  margin: 0;
}

.comment-textarea {
  width: 548rpx;
  height: 140rpx;
  padding: 10px;
  box-sizing: border-box;
  margin: 0 auto;
  border: 1px solid #ccc;
}

.comment-placeholder {
  line-height: 140rpx;
  text-align: center;
  color: #ccc;
  font-size: 14px;
}

.comment-btn {
  font-size: 18px;
  color: #fff;
  height: 100rpx;
  width: 548rpx;
  line-height: 50px;
  border-radius: 4rpx;
}

/** by youyou end **/

/* by Ethan start */

/* 公用最外层 */

.container {
  width: 100%;
  min-height: 100%;
  color: #333;
  box-sizing: border-box;
  padding: 20rpx;
  position: relative;
}

.txt-cnt {
  text-align: center;
}

.txt-rgt {
  text-align: right;
}

.main-color {
  color: #f1604f;
}

.rgt-arr {
}

/* 公用最外层end */

/* 公用块 */

.main-wrap {
  background: #fff;
  border-radius: 4rpx;
  box-shadow: 0 5px 10px rgba(0, 0, 0, 0.06);
  overflow: hidden;
  position: relative;
}

.main-wrap + .main-wrap {
  margin-top: 20rpx;
}

.main-wrap 
.remark {
  width: 100%;
  font-size: 24rpx;
  color: #999;
  background: #fafafa;
  padding: 18rpx;
}

.xs-font {
  font-size: 24rpx;
}

/* 公用外层end */

.dp-flex {
  display: flex;
  align-items: center;
}

.dp-flex.flex-one > view, .dp-flex.flex-one > button {
  flex: 1;
}
.dp-flex .flex-two {
  flex: 2;
}
.dp-flex.jc-sb {
  justify-content: space-between;
}

.dp-flex.jc-cnt {
  justify-content: center;
}

.jc-cen {
  justify-content: center;
}

.dp-flex.ai-fs {
  align-items: flex-start;
}

/* 公用样式end */

/* 导航区 */

.main-nav {
  color: #666;
  padding: 57rpx 40rpx 37rpx;
}

.main-nav > view {
  font-size: 24rpx;
  text-align: center;
}

.main-nav > view:not(:last-of-type) {
  margin-right: 80rpx;
}

.main-nav > view image {
  width: 60rpx;
  height: 28rpx;
  margin-bottom: 25rpx;
}

/* 导航区end */

/* 表单样式 */

.main-tit {
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
}

.main-tit.ad-b-bor {
  padding-bottom: 30rpx;
  border-bottom: 1px solid #ccc;
}

.main-form {
  background: #fff;
  position: relative;
}

.main-form.pad-lft {
  padding-left: 20rpx;
}

.main-form .item {
  padding: 25rpx 0;
  border-bottom: 1px solid #ccc;
}

.main-form .item.no-bor-b {
  border-bottom: 0;
}

.main-form .lab {
  width: 140rpx;
  color: #666;
  margin-right: 40rpx;
}

.main-form .sub-lab {
  font-size: 24rpx;
  color: #999;
}

.main-form input {
}

.main-form .info {
  /* color: #BBB; */
}

/* 搜索框 */

.search {
  position: relative;
}

.search input {
  font-size: 28rpx;
  text-align: center;
  background: #f2f2f2;
  border-radius: 30rpx;
  padding: 10rpx 20rpx;
}

/* placeholder */

.ph {
  color: #bbb;
}

/* 搜索框end */

/* 主按钮 */

.btn-wrap, .btn-wrap-two {
  width: 100%;
  position: absolute;
  left: 0;
  bottom: 0;
  padding: 20rpx;
  box-sizing: border-box;
}

.btn-wrap-two {
  display: flex;
  align-items: center;
}

.btn-wrap-two button {
  flex: 1;
  flex-direction: column;
  padding: 4px;
}

.btn-wrap-two .sub-btn {
  margin-left: 10rpx;
}

.btn-wrap button {
  padding: 24rpx;
}

.main-btn, .sub-btn {
  width: 100%;
  text-align: center;
  border-radius: 4rpx;
  line-height: normal;
}

.main-btn {
  width: 100%;
  font-size: 32rpx;
  color: #fff;
  text-align: center;
  background: #f1604f;
  border-radius: 4rpx;
}

.sub-btn {
  background: #fff;
  color: #f1604f;
}

.tips {
  font-size: 24rpx;
  color: #666;
  text-align: center;
  margin-bottom: 30rpx;
}

.tips.sub {
  color: #fff;
}

/* 主按钮end */

/* 遮罩 */

.mask {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  background: rgba(0, 0, 0, 0.5);
  padding: 20rpx;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 500;
}

/* 遮罩end */

/* 表单样式end */

/* 自定义picker start */

.custom-picker-wrap {
  position: absolute;
  left: 0;
  top: 0;
}

.custom-picker {
  width: 100%;
  position: fixed;
  bottom: -300px;
  background: #fff;
  z-index: 999;
}

.custom-picker.enter {
  bottom: 0;
  -webkit-transition: bottom 0.3s ease-in-out;
  transition: bottom 0.3s ease-in-out;
}

.custom-picker.leave {
  bottom: -300px;
  -webkit-transition: bottom 0.3s ease-in-out;
  transition: bottom 0.3s ease-in-out;
}

.custom-picker-column {
  text-align: center;
  line-height: 50px;
}

.custom-picker-head {
  display: flex;
  padding: 24rpx;
  background: #fff;
  border-bottom: 1px solid #ebebeb;
}

.custom-picker-head text {
  flex: 1;
}

.custom-picker-head .confirm {
  text-align: right;
}

.pick-cnt {

}
.pick-item > view:first-of-type {
  font-size: 32rpx;
}

.pick-btn {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 48rpx;
  height: 48rpx;
  line-height: 48rpx;
  font-size: 40rpx;
  border: 1px solid #ccc;
  border-radius: 50%;
}

.pick-val {
  color: #666;
  font-size: 36rpx;
  margin: 40rpx;
}
/* 附加选项 */

.sub-term {
  color: #bbb;
  padding: 20rpx 0;
}

.sub-term .dp-flex {
  justify-content: center;
}

.sub-term .dp-flex:not(:last-of-type) {
  border-right: 1px solid #ccc;
}

.sub-term image {
  width: 19rpx;
  height: 28rpx;
  margin-right: 20rpx;
}

/* 自定义picker end */

/* 车辆类型 start */

.car-type {
  position: relative;
  padding: 30rpx 0 60rpx;
}

.car-type > view {
  text-align: center;
}

.car-type image {
  width: 150rpx;
  max-height: 66rpx;
}

.car-type-txt {
  width: 110rpx;
  margin: 16rpx auto 30rpx;
}

.car-type .active .car-type-txt {
  color: #fff;
  border-radius: 15rpx;
  background: #f1604f;
}

.car-type .remarks {
  width: 100%;
  font-size: 24rpx;
  color: #999;
  text-align: center;
  background: #fafafa;
  padding: 18rpx;
  position: absolute;
  left: 0;
  bottom: 0;
}

/* 车辆类型 end */

/* 起终点伪类 start */

.start, .end {
  position: relative;
}

.start:before, .end:before {
  content: "";
  width: 12rpx;
  height: 12rpx;
  border-radius: 50%;
  position: absolute;
  left: 10rpx;
  top: 40%;
}

.start:before {
  background: #44bea3;
}

.end:before {
  background: #f1604f;
}

/* 起终点伪类 end */

/* 悬浮按钮 */

.hover-btn {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 60rpx;
  height: 60rpx;
  color: #fff;
  border-radius: 50%;
  padding: 0;
  background: #f1604f;
  box-shadow: 2px 2px 6px rgba(241, 96, 79, 0.5);
}

/* 包车旅游公用样式 */

.travel .item {
  color: #999;
  font-size: 24rpx;
}

.travel .main-form {
  padding-left: 30rpx;
}

.travel .pro-detail {
  padding: 10rpx 0 30rpx;
}

.travel .pro-detail > view:last-of-type {
  flex: 2;
  padding-right: 30rpx;
}

.travel .pro-img {
  margin-right: 30rpx;
}

.travel .pro-img image {
  width: 214rpx;
  height: 184rpx;
  border-radius: 10rpx;
  box-shadow: 4rpx 0px 16rpx rgba(0, 0, 0, 0.2);
}

.travel .name {
  font-size: 32rpx;
  color: #333;
  margin-bottom: 34rpx;
}

.travel .day {
  padding-left: 20rpx;
  border-left: 1px solid #ccc;
}

.travel .score {
  margin: 0 15rpx 0 0;
}

.travel .score image {
  width: 24rpx;
  height: 24rpx;
}

.travel .score image + image {
  margin-left: 4rpx;
}

.travel .collect {
  margin-top: 20rpx;
}

.travel .price {
  width: 100%;
}

.travel .intro {
  font-size: 22rpx;
}

.travel .recommend {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 32rpx;
  height: 32rpx;
  color: #fff;
  border-radius: 6rpx;
  background: #f1604f;
  margin-right: 15rpx;
}

.travel .hover-btn {
  width: 120rpx;
  height: 120rpx;
  flex-direction: column;
  position: fixed;
  right: 50rpx;
  bottom: 8%;
}

.travel .hover-btn image {
  width: 40rpx;
  height: 40rpx;
}

/* 用户评价 */

.comments-wrap {
  font-size: 28rpx;
  margin-bottom: 20rpx;
}

.comments-tit {
  padding: 30rpx 0;
  margin: 0 50rpx;
  border-bottom: 1px solid #ccc;
}

.comments-wrap scroll-view {
  padding: 0 50rpx;
  box-sizing: border-box;
}

.comments {
  margin: 30rpx auto 20rpx;
}

.comments-user image {
  width: 74rpx;
  height: 74rpx;
  margin-right: 30rpx;
  border-radius: 50%;
}

.user-info view {
  color: #666;
}

.user-info text {
  font-size: 20rpx;
  color: #909090;
}

.star image {
  width: 18rpx;
  height: 18rpx;
  margin: 0;
}

.comments-info {
  margin: 20rpx 0 0 10rpx;
}

