/* 登录、注册 end */

/* 弹出层 start */
.mask-no-padding {
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  background: rgba(0, 0, 0, .5);
  position: fixed;
  left: 0;
  top: 0;
  z-index: 500;
}
.mask-title{
  height:120rpx;
  padding:44rpx 30rpx;
  box-sizing:border-box;
}
.mask-item{
  height:160rpx;
  padding-left:40rpx;
  padding-top:44rpx;
  box-sizing:border-box;
}
.mask-item.active{
  background:rgba(0, 0, 0, 0.1);
}
/* 弹出层 end */

.tab-head{
