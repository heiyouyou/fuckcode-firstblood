<!--pages/my/CarOwnerCertification/license/license.wxml-->
<view class="displayf flexdirec"> 
    <view class="mt10">
        <view class="bgw my-box-shadow mb10">
            <view class="h50 displayf altc color-666 pl20 border-bottom-ccc">
                <text class="fz14 input-des-text">汽车品牌</text>
                <input class="fz14 color-333" placeholder="请输入您的汽车品牌" placeholder-class="color-BBB"/>
            </view>
            <view class="h50 displayf altc color-666 pl20 border-bottom-ccc">
                <text class="fz14 input-des-text">汽车型号</text>
                <input class="fz14 color-333" placeholder="请输入您的车型，如：SUV" placeholder-class="color-BBB"/>
            </view>
            <view class="h50 displayf altc color-666 pl20">
                <text class="fz14 input-des-text">车牌号</text>
                <input class="fz14 color-333" placeholder="请输入您的车牌号" placeholder-class="color-BBB"/>
            </view>
        </view>
        <view class="bgw my-box-shadow mb10">
            <view class="h50 displayf altc color-666 pl20 border-bottom-ccc">
                <text class="fz14 input-des-text">车龄</text>
                <input class="fz14 color-333" type="number" placeholder="请输入您的车龄" placeholder-class="color-BBB"/>
            </view>
            <view class="h50 displayf altc color-666 pl20">
                <text class="fz14 input-des-text">驾龄</text>
                <input class="fz14 color-333" type="number" placeholder="请输入您的驾龄" placeholder-class="color-BBB"/>
            </view>
        </view>
        <view class="bgw my-box-shadow mb10">
            <view class="h50 lh50 border-bottom-ccc color-666 parl20">
                <text class="fz14 input-des-text">车辆类型</text>
                <text class="fz14 color-333">舒适型</text>
                <view class="fr">
                    <image class="choice-size" src="../../../imgs/common/choice@2x.png"></image>
                </view>
            </view>
            <view class="h50 lh50 border-bottom-ccc color-666 parl20">
                <text class="fz14 input-des-text">儿童座椅</text>
                <text class="fz14 color-333">舒适型</text>
                <view class="fr">
                    <image class="choice-size" src="../../../imgs/common/choice@2x.png"></image>
                </view>
            </view>
            <view class="h50 displayf altc color-666 pl20 border-bottom-ccc">
                <text class="fz14 input-des-text">乘客上限</text>
                <input class="fz14 color-333" type="number" placeholder="请输入最多承载人数" placeholder-class="color-BBB"/>
            </view>
            <view class="h50 displayf altc color-666 pl20">
                <text class="fz14 input-des-text">行李数量</text>
                <input class="fz14 color-333" type="number" placeholder="请输入最多承载行李数量" placeholder-class="color-BBB"/>
            </view>
        </view>
        <view class="bgw my-box-shadow mb10 patrbl20">
            <view class="displayf jscb mb45">
                <view class="flex1 mr5">
                    <text class="fz14 color-666">手持护照实例</text>
                    <view class="mt15">
                        <image class="license-img bg-F1604F"></image>
                    </view>
                </view>
                <view class="flex1 ml5">
                    <text class="fz14 color-666">上传手持护照</text>
                    <view class="mt15 border-dashed-ccc">
                        <image class="license-img"></image>
                    </view>
                </view>
            </view>
            <view class="displayf jscb mb45">
                <view class="flex1 mr5">
                    <text class="fz14 color-666">上传全险</text>
                    <view class="mt15">
                        <image class="license-img bg-F1604F"></image>
                    </view>
                </view>
                <view class="flex1 ml5">
                    <text class="fz14 color-666">上传驾照</text>
                    <view class="mt15 border-dashed-ccc">
                        <image class="license-img"></image>
                    </view>
                </view>
            </view>
            <view class="displayf jscb mb45">
                <view class="flex1 mr5">
                    <text class="fz14 color-666">上传全险</text>
                    <view class="mt15">
                        <image class="license-img bg-F1604F"></image>
                    </view>
                </view>
                <view class="flex1 ml5">
                    <text class="fz14 color-666">上传驾照</text>
                    <view class="mt15 border-dashed-ccc">
                        <image class="license-img"></image>
                    </view>
                </view>
            </view>
        </view>
    </view>
    <view class="w710 mb10 mt15 mauto">
        <view class="tc mb15 color-666">
            提交资料代表同意
            <text class="color-F1604F">Meet使用协议</text>
            和
            <text class="color-F1604F">隐私条款</text>
        </view>
        <button class="save-btn">下一步</button>
    </view>
</view>
