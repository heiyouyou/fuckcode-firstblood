                                    <text class="fz12 color-666" style="margin-left:35rpx;">（普通会员）</text>
                                    <view class="pa navigator-choice" style="right:0px;">
                                        <image class="choice-size" src="../../imgs/common/choice@2x.png"></image>
                                    </view>
                            </view> 
                            <view class="lh10">
                                <image class="start-img displayib" src="../../imgs/common/score-star-red.png" wx:for="{{starNum}}"></image>
                            </view>
