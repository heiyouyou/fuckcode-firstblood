<!--pages/components/num-of-people.wxml-->
<view class='custom-picker-wrap'>
  <view class='mask' wx:if="{{show}}"></view>
  <view class='custom-picker {{show ? "enter" : "leave"}}'>
    <view class='custom-picker-head'>
      <text class='cancle' bindtap='_onCancle'>取消</text>
      <text class='confirm' bindtap='_onConfirm'>确定</text>
    </view>
  </view>
</view>
